    "@parcel/compressor-gzip": "^2.13.3",
    "@parcel/reporter-bundle-analyzer": "^2.13.3",
    "@parcel/transformer-sass": "^2.13.3",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "parcel": "^2.13.3",
    "prettier": "^3.4.2",
    "process": "^0.11.10",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
